acp = function(data_in){
result = list()
#Filtre
result$raw_data = na.omit(data_in[sapply(data_in, is.numeric)])
result$matrix = data.matrix(result$raw_data, rownames.force = NA)
#Moyenne
result$mean = apply(result$matrix, 2, mean)
#Variance/co-variance
result$var = t(result$matrix) %*% result$matrix
#Centrage
result$center = sapply()
#Retour
return(result)
}
